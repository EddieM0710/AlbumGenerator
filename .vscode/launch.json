{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb-mac) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/subprojects/Build/wx_cmake_template_core/main.app",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "Build with CMake"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/sandbox/Development/StampTool/build/StampTool",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(msvc) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\build\\subprojects\\Build\\wx_cmake_template_core\\Debug\\main.exe",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "Build with CMake"
        }
    ]
}
            $('.infoBlock').on('touchstart', function (e) {
                var link = $(".jscriptToggleInfoBlock", this);
                if (link.hasClass('hover active')) {
                    link.css("display", "none");
                    link.removeClass('hover active');
                    link.addClass('nonhover inactive');
                } else {
                    link.css("display", "block");
                    link.removeClass('nonhover inactive');
                    link.addClass('hover active');
                }
            });

            $(".infoBlock").hover(function () {
                $(".jscriptToggleInfoBlock", this).css("display", "block");
            }, function () {
                $(".jscriptToggleInfoBlock", this).css("display", "none");
